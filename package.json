{
  "name": "tree-sitter-mdx",
  "version": "1.0.0",
  "description": "MDX grammar for node-tree-sitter",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test"
  },
  "keywords": [
    "parser",
    "lexer"
  ],
  "author": "Javier López Úbeda",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.16.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "tree-sitter": [
    {
      "scope": "source.mdx",
      "file-types": [
        "mdx"
      ],
      "injection-regex": "^(mdx)$"
    }
  ]
}
